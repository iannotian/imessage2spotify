{
  "name": "imessage2spotify",
  "version": "1.1.0",
  "main": "index.js",
  "author": "Ian MacFarlane",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node --enable-source-maps dist/index.js",
    "start:dev": "nodemon src/index.ts",
    "build": "rimraf dist && tsc --build && copyfiles -u 1 src/views/*.ejs dist",
    "postinstall": "npm run build",
    "migrate:create": "mikro-orm migration:create",
    "migrate:run": "mikro-orm migration:up",
    "migrate:heroku": "npm run migrate:run",
    "heroku-postbuild": "npm run build && npm run migrate:heroku",
    "test": "jest"
  },
  "dependencies": {
    "@mikro-orm/core": "^4.5.9",
    "@mikro-orm/postgresql": "^4.5.9",
    "@mikro-orm/reflection": "^4.5.9",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "got": "^11.8.2",
    "rollbar": "^2.24.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^4.5.9",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "copyfiles": "^2.4.1",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "volta": {
    "node": "16.12.0"
  },
  "engines": {
    "node": "16.12.0"
  }
}
